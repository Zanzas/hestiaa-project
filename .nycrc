{
    "reporter": [
        "lcov",
        "html",
        "text"
    ],
    "include": [
        "lib/*/**/*.js"
    ],
    "exclude": [
        "lib/controllers/**",
        "lib/bootstrap/**",
        "lib/middlewares/**",
        "test/**"
    ],
    "watermarks": {
      "lines": [85, 92],
      "functions": [85, 92],
      "branches": [69, 76],
      "statements": [85, 92]
    },
    "report-dir": "./.coverage",
    "all": true,
    "per-file": true,
    "lines": 0,
    "functions": 0,
    "check-coverage": true
}
